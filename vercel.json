{
    "version": 2,
    "builds": [
      {
        "src": "wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/wsgi.py"
      }
    ],
    "env": {
      "DJANGO_SECRET_KEY": "@django-secret-key",
      "DJANGO_ALLOWED_HOSTS": "localhost, vercel.com",
      "DATABASE_URL": "@your-database-url" 
    },
    "installCommand": "pip install -r requirements.txt"
  }
  